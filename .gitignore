######################################
# Laravel / PHP Project .gitignore
######################################

# Node packages
node_modules/

# Build outputs
public/build/
public/hot/
public/mix-manifest.json

# Storage symlink target and runtime storage
public/storage/
storage/*.key
storage/logs/*
storage/debugbar/*
storage/app/public/*
storage/framework/cache/*
storage/framework/sessions/*
storage/framework/testing/*
storage/framework/views/*

# Bootstrap cache
bootstrap/cache/*

# Composer vendor
vendor/
composer.phar

# Environment
.env
.env.*
!.env.example

# Testing
.phpunit.result.cache
coverage/
tests/coverage/

# Editors & IDE
.idea/
.vscode/
/.fleet
*.swp
*.swo
*~

# Logs
*.log

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
desktop.ini

# Redis dump/logs (if present locally)
*.rdb
dump.rdb
appendonly.aof
redis.log

# Homestead / local auth files
Homestead.json
Homestead.yaml
auth.json

# Package manager debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Backups / temp
*.bak
*.backup
*.tmp

# Security keys
*.key
*.pem

# Local-only config
config/local/


